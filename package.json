{
  "name": "dagula-gateway",
  "version": "1.4.0",
  "description": "An IPFS Gateway for Cloudflare Workers that uses Dagula.",
  "main": "src/worker.js",
  "type": "module",
  "scripts": {
    "lint": "standard",
    "build": "npm run build:templates && npm run build:bundle",
    "build:templates": "handlebars templates/*.handlebars -c @web3-storage/handlebars/runtime.js -f templates/bundle.cjs",
    "build:bundle": "esbuild --format=esm --bundle --outfile=./dist/worker.mjs ./src/index.js",
    "dev": "npm run build && miniflare dist/worker.mjs --watch --debug -m --global-timers",
    "test": "npm run build && ava"
  },
  "keywords": [
    "gateway",
    "ipfs",
    "cloudflare",
    "workers",
    "dagula",
    "ipfs",
    "libp2p"
  ],
  "author": "Alan Shaw",
  "license": "Apache-2.0 OR MIT",
  "dependencies": {
    "@chainsafe/libp2p-noise": "^7.0.1",
    "@ipld/car": "^4.1.4",
    "@libp2p/mplex": "^1.2.1",
    "@libp2p/peer-id-factory": "^1.0.17",
    "@web3-storage/handlebars": "^1.0.0",
    "bytes": "^3.1.2",
    "cf-libp2p-ws-transport": "^1.1.0",
    "dagula": "^2.0.6",
    "ipfs-core": "^0.15.4",
    "libp2p": "^0.37.3",
    "magic-bytes.js": "^1.0.12",
    "mrmime": "^1.0.1",
    "multiformats": "^9.7.0",
    "timeout-abort-controller": "^3.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "ava": "^4.3.1",
    "esbuild": "^0.14.49",
    "miniflare": "^2.6.0",
    "standard": "^17.0.0",
    "typescript": "^4.7.4",
    "uint8arrays": "^3.1.0",
    "wrangler": "^2.0.22"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web3-storage/dagula-gateway.git"
  },
  "bugs": {
    "url": "https://github.com/web3-storage/dagula-gateway/issues"
  },
  "homepage": "https://github.com/web3-storage/dagula-gateway#readme"
}
